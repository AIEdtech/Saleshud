{
  "name": "saleshud",
  "productName": "SalesHud",
  "version": "1.0.0-beta",
  "description": "AI-powered sales intelligence",
  "main": "dist/main/main.js",
  "author": "Gaurav Gupta",
  "license": "SalesHud Proprietary License",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "build:main": "webpack --config webpack.main.config.js --mode production",
    "build:renderer": "webpack --config webpack.renderer.config.js --mode production",
    "build:preload": "webpack --config webpack.preload.config.js --mode production",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "start": "electron dist/main/main.js",
    "dist": "npm run build && electron-builder --config electron-builder.config.js",
    "dist:mac": "npm run build && electron-builder --mac --config electron-builder.config.js",
    "dist:win": "npm run build && electron-builder --win --config electron-builder.config.js",
    "dist:linux": "npm run build && electron-builder --linux --config electron-builder.config.js",
    "dist:all": "npm run build && electron-builder --mac --win --linux --config electron-builder.config.js",
    "clean": "rimraf dist release",
    "clean:all": "rimraf dist release node_modules/.cache",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "generate:assets": "node scripts/generate-icons.js",
    "validate:assets": "node scripts/generate-icons.js validate",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.16",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "electron-updater": "^6.1.7",
    "framer-motion": "^11.0.3",
    "lucide-react": "^0.312.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.24.0",
    "@babel/plugin-transform-object-rest-spread": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@electron/notarize": "^2.2.1",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^20.19.13",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "buffer": "^6.0.3",
    "chalk": "^4.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "cssnano": "^6.1.0",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-webpack-plugin": "^4.1.0",
    "events": "^3.3.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "fs-extra": "^11.2.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.1",
    "node-loader": "^2.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.33",
    "postcss-loader": "^8.1.1",
    "postcss-reporter": "^7.1.0",
    "process": "^0.11.10",
    "react-refresh": "^0.14.0",
    "rimraf": "^5.0.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "stream-browserify": "^3.0.0",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "util": "^0.12.5",
    "vitest": "^1.2.1",
    "wait-on": "^7.2.0",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-hot-middleware": "^2.26.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saleshud/saleshud-app.git"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "sales",
    "ai",
    "overlay",
    "transcription",
    "real-time"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ]
}
